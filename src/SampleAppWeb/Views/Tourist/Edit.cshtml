@using SampleAppWeb.Uow.Models
@model Tourist

@{
    ViewData["Title"] = "Tourist edit Page";
}



<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nome</th>
                <th scope="col">Mail</th>
                <th scope="col">Location</th>
                <th scope="col">CreatedAt</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                    <td>@Model.TouristName</td>
                    <td>@Model.TouristEmail</td>
                    <td>@Model.TouristLocation</td>
                    <td>@Model.Createdat</td>
                </tr>
        </tbody>
    </table>
</div>